#site-head
  background white
  transition padding ease 0.5s
  position relative
  width 100%
  z-index 1
  h1, h2, h3
    margin 0
    border 0
    padding 0
    font-family headerFontFamily
    text-align center
  h3
    font-size 35px
    margin 20px
  &.tag
    background transparent
    h1
      color white
    h2
      color darken(white, 20%)
    h3
      text-align center
      color white
      font-size 35px
      margin 20px